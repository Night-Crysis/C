#include <iostream>
#include <stdbool.h>

using namespace std;


int main()
{

	int n,p,d;
	bool isPrime;

	cout << "Enter N\n";
	cin >> n;
	

	for (p = 2; p <= n; p++)
	{
		isPrime = true;
		for (d = 2; d < p ; d++)
		if (p % d == 0)
			isPrime = false;
		if (isPrime != false)
			cout << p << "\n";
	}
	return 0;
}




